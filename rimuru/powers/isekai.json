            "condition":{
                "type":"origins:and",
                "conditions":[
                {
                    "type":"origins:resource",
                    "resource": "*:*_ate_seed",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type":"origins:resource",
                    "resource": "*:*_demon_slime",
                    "comparison": "==",
                    "compare_to": 0
                }
                ]
            }
        }
    },
    "demon_sacrifice":{
