                "item":"minecraft:milk_bucket"
            }
        },
        "condition":{
            "type":"origins:resource",
            "resource": "*:*_ready_demon_slime",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "no_die":{
        "type": "origins:prevent_death",
        "condition":{
            "type":"origins:resource",
            "resource": "*:*_ready_demon_slime",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "nb_kill":{
